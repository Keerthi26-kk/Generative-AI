    "\"\"\"def load_and_preprocess_data(file_path):\n",
    "    df=pd.read_csv(file_path)\n",
    "    df.fillna(df.median(),inplace=True)\n",
    "    df['Customer demographics'].fillna('Unknown',inplace=True)\n",
    "    df['disruption']= ((df['Availability']==0)&\n",
